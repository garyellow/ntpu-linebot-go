# LINE Bot Configuration (Required)
LINE_CHANNEL_ACCESS_TOKEN=your_access_token_here
LINE_CHANNEL_SECRET=your_channel_secret_here

# LLM Configuration (Optional - at least one API key required for NLU features)
# Enables NLU intent parsing and query expansion
# Get Gemini API key from: https://aistudio.google.com/apikey
# Get Groq API key from: https://console.groq.com/keys
#GEMINI_API_KEY=your_gemini_api_key_here
#GROQ_API_KEY=your_groq_api_key_here

# LLM Provider Selection (Optional)
# Primary provider (default: gemini)
#LLM_PRIMARY_PROVIDER=gemini
# Fallback provider (default: groq)
#LLM_FALLBACK_PROVIDER=groq

# LLM Model Configuration (Optional - defaults shown)
#GEMINI_INTENT_MODEL=gemini-2.5-flash
#GEMINI_INTENT_FALLBACK_MODEL=gemini-2.0-flash-lite
#GEMINI_EXPANDER_MODEL=gemini-2.5-flash
#GEMINI_EXPANDER_FALLBACK_MODEL=gemini-2.0-flash-lite
#GROQ_INTENT_MODEL=llama-3.1-8b-instant
#GROQ_INTENT_FALLBACK_MODEL=llama-3.3-70b-versatile
#GROQ_EXPANDER_MODEL=llama-3.1-8b-instant
#GROQ_EXPANDER_FALLBACK_MODEL=llama-3.3-70b-versatile

# Server Configuration
#PORT=10000
#LOG_LEVEL=info
#SHUTDOWN_TIMEOUT=30s

# Data Configuration
#DATA_DIR=/data
#CACHE_TTL=168h

# Scraper Configuration
#SCRAPER_TIMEOUT=60s
#SCRAPER_MAX_RETRIES=5

# Warmup Configuration
# Available modules: sticker, id, contact, course, syllabus
# Note: syllabus requires LLM API key to be set for smart search
#WARMUP_MODULES=sticker,id,contact,course

# Webhook Configuration
#WEBHOOK_TIMEOUT=60s

# Rate Limit Configuration
#USER_RATE_LIMIT_TOKENS=6
#USER_RATE_LIMIT_REFILL_RATE=0.2
#LLM_RATE_LIMIT_PER_HOUR=50
#GLOBAL_RATE_LIMIT_RPS=100
